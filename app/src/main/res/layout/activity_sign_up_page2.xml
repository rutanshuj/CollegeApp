<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_sign_up_page2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.admin.collegeadg.SignUpPage2"
    android:background="@color/white">

    <TextView
        android:text="@string/ClickHere"
        android:clickable="true"
        android:onClick="clickhere"
        android:textColor="#0000ff"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="14dp"
        android:id="@+id/textView4"
        android:layout_below="@+id/congrats"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/congrats"
        android:text="Congratulations, you have successfully
created an account."
        android:layout_marginTop="143dp"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

</RelativeLayout>
